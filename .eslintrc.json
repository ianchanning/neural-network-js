{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["prettier"],
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "prettier/prettier": "error",
        "quotes": ["error", "double"]
    }
}
